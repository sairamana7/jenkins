    agent any

    stages {
        stage('Hello') {
            steps {
                echo ' World'
            }
        }
        stage('hi') {
            steps {
                echo 'Hello '
            }
        }
        stage('done') {
            steps {
                echo ' World'
            }
        }
        stage('deployed') {
            steps {
                echo 'Hello '
            }
        }
        
    }
}
